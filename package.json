{
  "name": "br-experiments",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "predeploy:clean": "rimraf dist .clone",
    "predeploy:clone": "git clone --depth 1 https://github.com/gitgrimbo/br-experiments.git .clone",
    "predeploy:webpack": "npx webpack -p --config .clone/webpack.config.js",
    "predeploy:copy-dist": "copyfiles --verbose --up 1 .clone/dist/**/* dist",
    "predeploy:copy-experiments": "copyfiles --verbose --up 1 .clone/experiments/**/* dist",
    "predeploy:copy-parent-html": "node bin/move-file-cli.js dist/experiments/index.parent.html dist/index.html",
    "predeploy:dist": "run-s predeploy:webpack predeploy:copy-dist predeploy:copy-experiments predeploy:copy-parent-html",
    "predeploy": "run-s predeploy:clean predeploy:clone predeploy:webpack predeploy:copy-dist predeploy:copy-experiments predeploy:copy-parent-html",
    "deploy": "gh-pages -d dist",
    "start": "webpack-dev-server",
    "lint": "eslint experiments --ext .js,.jsx,.ts,.tsx"
  },
  "author": "Paul Grime",
  "license": "ISC",
  "devDependencies": {
