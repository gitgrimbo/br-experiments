{
  "name": "br-experiments",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "predeploy-clean": "rimraf dist .clone",
    "predeploy-clone": "git clone --depth 1 https://github.com/gitgrimbo/br-experiments.git .clone",
    "predeploy-webpack": "npx webpack -p --config .clone/webpack.config.js",
    "predeploy-make-dist": "copyfiles --verbose --up 1 .clone/dist/**/* .clone/experiments/**/* dist",
    "predeploy": "run-s predeploy-clean predeploy-clone predeploy-webpack predeploy-make-dist",
    "deploy": "gh-pages -d dist"
  },
  "author": "Paul Grime",
  "license": "ISC",
  "devDependencies": {
    "@babel/cli": "^7.4.3",
    "@babel/core": "^7.4.3",
    "@babel/preset-env": "^7.4.3",
    "@babel/preset-react": "^7.0.0",
    "babel-loader": "^8.0.5",
    "babel-polyfill": "^6.26.0",
    "copyfiles": "^2.1.0",
    "gh-pages": "^2.0.1",
    "glob": "^7.1.3",
    "make-dir-cli": "^2.0.0",
    "npm-run-all": "^4.1.5",
    "rimraf": "^2.6.3",
    "sharp": "^0.22.0",
    "superagent": "^5.0.2",
    "webpack": "^4.30.0",
    "webpack-cli": "^3.3.0",
    "webpack-dev-server": "^3.3.1"
  },
  "dependencies": {
    "react": "^16.8.6",
    "react-dom": "^16.8.6"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/gitgrimbo/br-experiments.git"
  },
  "bugs": {
    "url": "https://github.com/gitgrimbo/br-experiments/issues"
  },
  "homepage": "https://github.com/gitgrimbo/br-experiments#readme",
  "description": ""
}
