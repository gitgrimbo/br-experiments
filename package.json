{
  "name": "br-experiments",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "predeploy-clean": "rimraf dist && make-dir dist",
    "predeploy-copy": "copyfiles --up 1 experiments/index.html experiments/main.css experiments/main.js experiments/fonts/**/* experiments/roster-badge/**/* experiments/banner/**/* experiments/twitter-embed/**/* dist",
    "predeploy-addlog": "node bin/log.js dist/index.html",
    "predeploy": "run-s predeploy-clean predeploy-copy predeploy-addlog",
    "deploy": "gh-pages -d dist"
  },
  "author": "Paul Grime",
  "license": "ISC",
  "devDependencies": {
    "copyfiles": "^2.1.0",
    "gh-pages": "^2.0.1",
    "make-dir-cli": "^1.0.0",
    "npm-run-all": "^4.1.5",
    "rimraf": "^2.6.3",
    "superagent": "^4.1.0"
  },
  "dependencies": {},
  "repository": {
    "type": "git",
    "url": "git+https://github.com/gitgrimbo/br-experiments.git"
  },
  "bugs": {
    "url": "https://github.com/gitgrimbo/br-experiments/issues"
